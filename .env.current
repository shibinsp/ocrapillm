# Sentry Configuration for Staging
SENTRY_DSN=

# Environment
ENVIRONMENT=staging

# Frontend Sentry DSN
REACT_APP_SENTRY_DSN=

# Database Configuration for Staging
# DATABASE_URL=postgresql://username:password@staging-db:5432/ocrapillm_staging

# API Configuration
# MISTRAL_API_KEY=your-staging-mistral-api-key
# OPENAI_API_KEY=your-staging-openai-api-key
# GOOGLE_CLOUD_VISION_API_KEY=your-staging-google-cloud-vision-key

# File Upload Configuration
MAX_FILE_SIZE=50MB
UPLOAD_DIR=uploads
OUTPUT_DIR=outputs

# Stage-wise Configuration
STAGE=staging
DEBUG=false
LOG_LEVEL=INFO

# Performance Settings
MAX_WORKERS=2
TIMEOUT=300

# Security Settings
CORS_ORIGINS=https://staging.yourdomain.com
ALLOWED_HOSTS=staging.yourdomain.com